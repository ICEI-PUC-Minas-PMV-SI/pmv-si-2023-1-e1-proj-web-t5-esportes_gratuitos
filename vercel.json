{
  "version": 2,
  "builds": [{
    "src": "/src/api/server.js",
    "use": "@vercel/node"
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "/src/api/server.js": {
      "memory": 1024,
      "includeFiles": "/src/api/db.json"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path",
      "destination": "/api/server.js"
    },
    { "source": "/(.*)", "destination": "/$1" }
  ]
}